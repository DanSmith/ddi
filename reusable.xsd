			  			<xs:documentation>Identifies the difference between the date applied to the data as a whole and this specific item such as previous year's income or residence 5 years ago.</xs:documentation>
					</xs:annotation>
	      		</xs:element>
	    	</xs:sequence>
	    	<xs:attribute name="classificationLevel" type="CategoryRelationCodeType" use="optional">
	      		<xs:annotation>
					<xs:documentation>Indicates the type of relationship, nominal, ordinal, interval, ratio, or continuous. Use where appropriate for the representation type.</xs:documentation>
	      		</xs:annotation>
	    	</xs:attribute>
	   </xs:complexType>
   <xs:element name="TextRepresentation" type="TextRepresentationType" substitutionGroup="ValueRepresentation">
