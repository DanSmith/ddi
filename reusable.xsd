<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="ddi:reusable:0_1" xmlns="ddi:reusable:0_1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xhtml="http://www.w3.org/1999/xhtml" elementFormDefault="qualified" attributeFormDefault="unqualified">
	
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<xs:import namespace="http://www.w3.org/1999/xhtml" schemaLocation="ddi-xhtml.xsd"/>

<!-- BASIC TYPES -->	
    <xs:attributeGroup name="GLOBALS">
        <xs:attribute name="ID" type="xs:NCName"/>
        <xs:attribute ref="xml:lang"/>
    </xs:attributeGroup>

	<xs:complexType name="IdentifiedStringType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attributeGroup ref="GLOBALS"/>
				<xs:attribute name="translated" type="xs:boolean" default="false"/>
				<xs:attribute name="translatable" type="xs:boolean" default="true"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	<xs:simpleType name="BaseDateType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:dateTime xs:date xs:gYearMonth xs:gYear"/>
	</xs:simpleType>

	<xs:complexType name="DateType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Date" type="BaseDateType">
					<xs:annotation>
						<xs:documentation>A single point in time.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="StartDate" type="BaseDateType">
						<xs:annotation>
							<xs:documentation>Start of a date range.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndDate" type="BaseDateType">
						<xs:annotation>
							<xs:documentation>End of a date range.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Cycle" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Attribute to indicate the occurrence of this date range within a series of ranges.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
		<!-- Need C.V. for this, -->
		<xs:attribute name="calendar" type="xs:string" use="optional"/>
	</xs:complexType>

	<xs:complexType name="InternationalStringType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
		        <xs:attribute ref="xml:lang"/>
				<xs:attribute name="translated" type="xs:boolean" default="false"/>
				<xs:attribute name="translatable" type="xs:boolean" default="true"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- Right now, the subset is just block level elements.  Further discussion should take place as to exactly which subset of XHTML should be used. -->
	<xs:complexType name="StructuredStringType" mixed="true">
		<xs:annotation>
			<xs:documentation>Provides for string content, in which xhtml content can exist.</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="xhtml:BlkNoForm.mix"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="translated" type="xs:boolean" default="false"/>
		<xs:attribute name="translatable" type="xs:boolean" default="true"/>
	</xs:complexType>
	
	<!-- This is used for any structure that needs to be referenced else where. -->
	<!-- QUESTION: Is the full identification necessary (will it be referenced externally) or only internally?
		 If it is internal only, use just an ID. -->
	<xs:complexType name="IdentifiedStructuredStringType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Identification">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Content" type="StructuredStringType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="EmptyType">
		<xs:annotation>
			<xs:documentation>An empty complex type.</xs:documentation>
		</xs:annotation>
	</xs:complexType>

<!-- IDENTIFICATION TYPE -->
	<xs:element name="Identification" type="IdentificationType">
		<xs:unique name="OneChoice">
			<xs:selector xpath="*"/>
			<xs:field xpath="@type"/>				
		</xs:unique>	
	</xs:element>
	<xs:complexType name="IdentificationType">
		<xs:annotation>
			<xs:documentation>Type to provide an identification, agency, and version information. Used to identify any entity defined in the DDI. An entity can either be identified by a URN, or a combination of ID/Agency/Version. Set includes elements covering all instances of version Statement in DDI v.2.0</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="2">
				<xs:element name="URN" type="URNType">
					<xs:annotation>
						<xs:documentation>URN providing identification. Specifics of URN structre TBD.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="ID" type="xs:NCName">
						<xs:annotation>
							<xs:documentation>ID assigned to the entity by the agency.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IdentifyingAgency" type="xs:NCName">
						<xs:annotation>
							<xs:documentation>Reference to the agency maintaining the identification.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VersionNumber" type="xs:string" default="1.0">
						<xs:annotation>
							<xs:documentation>Version Number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VersionDate" type="DateType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date the version took place.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VersionResponsibility" type="xs:NCName" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reference to the organization responsible for the version change.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Affiliation" type="xs:NCName" minOccurs="0">
						<xs:annotation>
							<xs:documentation></xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VersionReason" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Textual description of the reason for a version change.</xs:documentation>
						</xs:annotation>
					</xs:element>					
				</xs:sequence>
			</xs:choice>
			<xs:element name="Name" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Name given the entity being identified.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ReferenceType">
		<xs:annotation>
			<xs:documentation>Type for referencing and identified entity, either by a URN, or the combination of ID/Agency/Version.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="2">
				<xs:element name="URN" type="URNType">
					<xs:annotation>
						<xs:documentation>URN providing identification. Specifics of URN structre TBD.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="ID" type="xs:NCName">
						<xs:annotation>
							<xs:documentation>ID assigned to the entity by the agency.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IdentifyingAgency" type="xs:NCName">
						<xs:annotation>
							<xs:documentation>Reference to the agency maintaining the identification.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VersionNumber" type="VersionType" default="1.0">
						<xs:annotation>
							<xs:documentation>Version Number. The version can be identified as lateBound, meaning that the latest version should be taken.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="isExternal" type="xs:boolean" default="false">
			<xs:annotation>
				<xs:documentation>Identifies that the reference is to an external source. If the value of this is try, than a URI must be provided.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="URI" type="xs:anyURI" use="optional">
			<xs:annotation>
				<xs:documentation>URI identification the location of an external reference.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="VersionType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="lateBound" type="xs:boolean" default="false"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="URNType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:anyURI">
				<xs:attribute name="type" type="xs:string" fixed="URN"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:element name="Citation" type="CitationType"/>
	<xs:complexType name="CitationType">
		<xs:annotation>
			<xs:documentation>Provides citation information for a study.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Title" type="InternationalStringType">
				<xs:annotation>
					<xs:documentation>Full authoritative title.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubTitle" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Secondary title to amplify.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AlternateTitle" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>An alternative title by which a data collection is commenly referred to or an abbreviation  for the title.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Do we need a specific structured type for identifying a creator? -->
			<xs:element name="Creator" type="InternationalStringType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Person, corporate body, or agency responsible for the data/documents's substantive and intellectual content.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Publisher" type="InternationalStringType">
				<xs:annotation>
					<xs:documentation>Person or organization with financial or administrative responsibility.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contributor" type="ContributorType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PublicationDate" type="DateType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Code list, build a new reusable type for this? -->
			<xs:element name="Language" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Should this have a controlled vocab? --><!-- Is this a specific class, or can we create a generic identifier type, that contains and id and a type?  Perhaps we create this type as generic, and the use restriction to limit the type list? -->
			<xs:element name="InternationalIdentifier" type="TypedStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Should this occur N times? -->
			<xs:element name="Copyright" type="InternationalStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="ContributorType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="InternationalStringType">
				<xs:attribute name="type" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<!-- Should this be a controlled vocab? -->
				<xs:attribute name="role" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Job title such as data collection staff, editor, research initiator etc.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<!-- Should this be a controlled vocab? -->
				<xs:attribute name="affiliation" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:element name="FundingInformation" type="FundingInformationType"/>
	<xs:complexType name="FundingInformationType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Agency" type="xs:NCName">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyAbbreviation" type="xs:string">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrantNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="role" type="xs:string" use="optional"/>
	</xs:complexType>
	
	<xs:element name="Embargo" type="EmbargoType"/>
	<xs:complexType name="EmbargoType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Identification"/>
			<xs:element name="Date" type="DateType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Reason" type="StructuredStringType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Agency" type="xs:NCName">
				<xs:annotation>
					<xs:documentation>Agency responsible for placing the embargo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EnforcementAgency" type="xs:NCName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Agency responsible for the enforcement of the embargo</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="LifecycleInformation" type="LifecycleInformationType"/>
	<xs:complexType name="LifecycleInformationType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LifecycleEvent" type="LifecycleEventType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="LifecycleEventType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Identification"/>
			<xs:element name="Date" type="DateType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Agency" type="xs:NCName">
				<xs:annotation>
					<xs:documentation>Agency responsible for the event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="StructuredStringType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type" type="xs:string" use="optional"/>
	</xs:complexType>
	
	<xs:element name="Universe" type="UniverseType"/>
	<xs:complexType name="UniverseType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="TopicalCoverageReference" type="ReferenceType">
					<xs:annotation>
						<xs:documentation>Reference to a previously defined topical coverage.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TopicalCoverage" type="TopicalCoverageType">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="SpatialCoverageReference" type="ReferenceType">
					<xs:annotation>
						<xs:documentation>Reference to a previously defined spatial coverage.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SpatialCoverage" type="GeographicCoverageType">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="TemporalCoverageReference" type="ReferenceType">
					<xs:annotation>
						<xs:documentation>Reference to a previously defined temporal coverage.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TemporalCoverage" type="TemporalCoverageType">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="Label" type="LabelType"/>
	<xs:complexType name="LabelType">
		<xs:annotation>
			<xs:documentation>Provides for a string content with the a type indentifier.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="InternationalStringType">
				<xs:attribute name="geographyID" type="xs:NCName" use="optional">
					<xs:annotation>
						<xs:documentation>References a specific geographic location defined in the Spatial coverage of a universe. The value here is one of the GeographyCode/Value(s).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<!-- Need vocabulary -->
				<xs:attribute name="language" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="year" type="BaseDateType" use="optional">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="type" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="maxLength" type="xs:integer" use="optional">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

<!-- TOPICAL COVERAGE -->
	<xs:complexType name="TopicalCoverageType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Identification"/>
			<!-- Need complete list of controlled vocab, then change type to restricted type. -->
			<xs:element name="Subject" type="TypedStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Keyword" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Need complete list of controlled vocab, then change type to restricted type. -->
			<!-- Do we want to have a common Language type? -->
			<!-- Taking out, since the language seems like it would be specific to the keywords, and is inlcuded with xml:lang
			<xs:element name="Language" type="TypedStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>-->
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TypedStringType">
		<xs:annotation>
			<xs:documentation>Provides for a string content with the a type indentifier.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="InternationalStringType">
				<xs:attribute name="type" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation>Identifies the type of this string.  This type attribute can be restricted to use a controlled vocabulary.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	<xs:element name="Software" type="SoftwareType"/>
	<xs:complexType name="SoftwareType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="StructuredStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="GLOBALS"/>
		<xs:attribute name="version" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="package" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="product" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="date" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

<!-- GEOGRAPHIC COVERAGE -->
	<xs:complexType name="GeographicCoverageType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Identification"/>
			<xs:element name="BoundingBox" type="BoundingBoxType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Bounding Box is a specific form of a geographic polygon that describes the full extent of the geographic cover using a four-sided box constructed of 2 longitudes and 2 latitudes. It is used by systems that search for geography by coordinates and is compatible with the description and structure found in FGDC and other geographic metadata structures.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="StructuredStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The purpose of this element is to provide text that will map to the Dublin Core element Geographic Cover.  One or more descriptions of this geography will also be designated in Row 29 - GeographyName (as part of Geography > GeographyValues) but this element is different in two ways: 1) it is designed to be more detailed and descriptive than GeographyName, which matches with the code given in Row 26 - GeographyCode, and 2) it can describe a geographic area that is a grouping of several geographies (e.g. a study that covers 3 countries, each of which would be given its own Geography > GeographyValues > GeographyName > GeographyCode).  Among other goals, this method is designed to ensure that overall geographic coverage is machine actionable separate from geographic levels.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StructureDescription" type="ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Points to a variable describing the geographic levels available in the data such as the variable "Summary Level" in U.S. Census data.  This reference is needed for assistance in programming.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LowestGeographicDescription" type="GeographicDescriptionCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The type and codification of geographic variables (point, line, polygon).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Geography" type="GeographyType">
					<xs:annotation>
						<xs:documentation>Generic Geography class, used for any level of geography, either overall coverage or levels for which data is provided in the study.  The first occurrence of this is Geography that describes the overall coverage area, which can also be distinguished by a Geography for which TopLevelGeography value is set at True. However, there may be multiple occurrences.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GeographyReference" type="ReferenceType">
					<xs:annotation>
						<xs:documentation>Reference to a previousely defined geography.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="GeographicDescriptionCodeType">
		<xs:annotation>
			<xs:documentation>Indicates the most descrete level identifed for a single case. Note that data can be collected at a geographic point (address) and reported as such in a protected file, and then aggregated to a polygon for a public file.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
   			<xs:enumeration value="Point">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
   			<xs:enumeration value="Polygon">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
   			<xs:enumeration value="Line">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
   			<xs:enumeration value="LinearRing">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="GeographyType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!-- Question: Do we need the full identification, or will just an ID do here? -->
			<xs:element ref="Identification"/>		
			<xs:element name="Level" type="GeographicLevelType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentGeography" type="ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Values" type="GeographyValuesType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GeographicTime" type="DateType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="hasSummaryData" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="topLevel" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="lowestLevel" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="exhaustiveCoverage" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="BoundingBoxType">
		<xs:annotation>
			<xs:documentation>Set of north, south, east, west coordinates defining a box which contains the geographic region.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NorthLatitude" type="LatitudeType">
				<xs:annotation>
					<xs:documentation>North latitude of the bounding box.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EastLongitude" type="LongitudeType">
				<xs:annotation>
					<xs:documentation>East longitude of the bounding box.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SouthLatitude" type="LatitudeType">
				<xs:annotation>
					<xs:documentation>South latitude of the bounding box.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WestLongitude" type="LongitudeType">
				<xs:annotation>
					<xs:documentation>West longitude of the bounding box.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="LongitudeType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="-180"/>
			<xs:maxInclusive value="180"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="LatitudeType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="-90"/>
			<xs:maxInclusive value="90"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="GeographicLevelType">
		<xs:annotation>
			<xs:documentation>Used to identify the geographic level being described.  The level can be identified by a code, given a name, and reference a variable that contains the values for the code.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="2">
				<xs:sequence>
					<!-- Is there a controlled vocabulary for this. -->
					<xs:element name="Code" type="xs:string">
						<xs:annotation>
							<xs:documentation>A numeric code identifying the geographic level (e.g. Multinational, Nation, Region, City, etc.).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Authority" type="xs:NCName">
						<xs:annotation>
							<xs:documentation>Reference to the authority defining the code value in this sequence used to identify the geographic level.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="Name" type="InternationalStringType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Textual description of the level code (e.g. "Nation", "Region").</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="VariableReference" type="ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="GeographyValuesType">
		<xs:annotation>
			<xs:documentation>Expresses the values of any geographical area (e.g. which nation is covered in the study).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CoverageLimitation" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Describes limitation of coverage within the parent geography (for example only places over 1,000 population).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VariableReference" type="ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A pointer to a variable in the study that describes the  particular geography - such as BLOCK.  At this level (i.e. within GeographyValuesType) it can only refer to a specific geographic level.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExternalAuthorityReference" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Link to an external resource that defines the code list.</xs:documentation>
				</xs:annotation>
			</xs:element>			
			<xs:element name="GeographyValue" type="GeographyValueType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="GeographyValueType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GeographyCode" type="GeographyCodeType">
				<xs:annotation>
					<xs:documentation>A reference to a standard geography code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GeographyName" type="InternationalStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Textual description of the particular geographic entity/code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BoundingPolygon" type="PolygonType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A closed plane figure bounded by three or more line segments which describes the geographic area.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExcludingPolygon" type="PolygonType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A closed plane figure bounded by three or more line segments which describes an area excluded from the geographical area.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="GeographyCodeType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="xs:NCName">
				<xs:annotation>
					<xs:documentation>Code value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Authority" type="xs:NCName">
				<xs:annotation>
					<xs:documentation>Code authority</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PolygonType">
		<xs:annotation>
			<xs:documentation>A closed plane figure bounded by three or more line segments.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Point" type="PointType" minOccurs="3" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A point consisting of a latitude and longitude.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GeographicTime" type="DateType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>			
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PointType">
		<xs:annotation>
			<xs:documentation>A geographic point consisting of a latitude and longitude.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Latitude" type="LatitudeType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Longitude" type="LongitudeType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>			
		</xs:sequence>
	</xs:complexType>

